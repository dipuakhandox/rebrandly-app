{
  "name": "rebrandly-express",
  "version": "0.0.4",
  "description": "Seamlessly connect your web server to Rebrandly so that you can re-use your domain name for both your app and your short links",
  "main": "src/index.js",
  "scripts": {
    "test": "nyc mocha --require babel-core/register tests",
    "build": "npm run lint && babel ./src -d ./lib",
    "lint": "eslint ./src",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rebrandly/rebrandly-express.git"
  },
  "keywords": [
    "middleware",
    "express",
    "links",
    "routing",
    "domains",
    "rebrandly",
    "branded"
  ],
  "author": "Rebrandly Team",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/rebrandly/rebrandly-express/issues"
  },
  "homepage": "https://github.com/rebrandly/rebrandly-express#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.3.4",
    "eslint": "^8.6.0",
    "jest": "^27.4.7",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
